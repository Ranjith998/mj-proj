pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('clone') {
            steps {
                echo 'Hello World clone'
            }
        }
        stage('test') {
            steps {
                echo 'Hello World test'
            }
        }
        stage('compile') {
            steps {
                echo 'Hello World compile'
            }
        }
        stage('build') {
            steps {
                echo 'Hello World build'
            }
        }
    }
}
